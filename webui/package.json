{
  "name": "langarr-webui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:next\"",
    "dev:next": "next dev",
    "dev:server": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' server/index.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "bowser": "^2.13.1",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "formik": "^2.4.9",
    "iso-639-1": "^3.1.5",
    "js-yaml": "^4.1.1",
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.6.0",
    "reflect-metadata": "^0.2.2",
    "swr": "^2.3.8",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'",
      "next lint"
    ]
  }
}